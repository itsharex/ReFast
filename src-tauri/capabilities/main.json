{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "Main application capabilities",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "core:window:allow-set-size",
    "core:window:allow-set-decorations",
    "core:window:allow-start-dragging",
    "core:window:allow-close"
  ]
}

